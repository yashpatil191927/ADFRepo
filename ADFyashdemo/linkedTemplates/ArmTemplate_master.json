{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFyashdemo"
		},
		"LS_SqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SqlDatabase'"
		},
		"SqlServerlinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerlinkedservice'"
		},
		"linkedservice_storagedemoyash_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice_storagedemoyash'"
		},
		"LS_BlobStorage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "Yash19127"
		},
		"LS_KV_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "Yash19127"
		},
		"LS_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Yash-KVDemo.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFyashdemo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SqlDatabase_connectionString": {
						"value": "[parameters('LS_SqlDatabase_connectionString')]"
					},
					"SqlServerlinkedservice_connectionString": {
						"value": "[parameters('SqlServerlinkedservice_connectionString')]"
					},
					"linkedservice_storagedemoyash_connectionString": {
						"value": "[parameters('linkedservice_storagedemoyash_connectionString')]"
					},
					"LS_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_BlobStorage_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_BlobStorage_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFyashdemo_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_KV_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_KV_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFyashdemo_ArmTemplate_0')]"
			]
		}
	]
}